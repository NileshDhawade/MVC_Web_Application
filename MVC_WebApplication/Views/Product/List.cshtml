<h1>
    this is my product list
</h1>
<table class="table">
    <thead>
        <tr>
        <th scope="col">id</th>
        <th scope="col">name</th>
        <th scope="col">price</th>
    </tr>

    </thead>
    
    
    
    <tb>
        @foreach (MVC_WebApplication.Models.Product item in ViewBag.prod)
        {
            <tr>
               <td>
                  @item.id
               </td>
               <td>
                  @item.name
               </td>
               <td>
                  @item.price
               </td>
            </tr>
            
        }
    </tb>
</table>